@{
	if (@ViewBag.CV == null)
	{
		<div class="container row row-cols-1 gy-5">
			<div class="col">
				<div id="1" class="align-content-center">
					@ViewBag.CV1;
				</div>
			</div>
			<div class="col">
				<div id="2" class="align-content-center">
					@ViewBag.CV2;
				</div>
			</div>
			<div class="col">
				<div id="3" class="align-content-center">
					@ViewBag.CV3;
				</div>
			</div>
			<div class="col">
				<div id="4" class="align-content-center">
					@ViewBag.CV4;
				</div>
			</div>
		</div>
	}
	else
	{
		<div class="container row row-cols-1 gy-5">
			<div class="col">
				@ViewBag.CV;
			</div>
		</div>
	}
}
